<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.graduate.seoil.sg_projdct">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="작심 성공"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme2"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".GoalMaking"></activity>
        <activity android:name=".AccountSetting" />
        <activity android:name=".PostAddActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".GroupActivity" />
        <activity android:name=".GroupInformation" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".group_index" />
        <activity android:name=".SettingActivity" />
        <activity
            android:name=".ResetPasswordActivity"
            android:parentActivityName=".LoginActivity" />
        <activity android:name=".MessageActivity" /> <!-- Firebase Login from "KOD Dev" -->
        <activity android:name=".GStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndexActivity" />
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".GStartActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".GStartActivity" />
        <activity
            android:name=".TemplateActivity"
            android:label="@string/title_activity_template" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".GroupRegistActivity" />
        <activity android:name=".StartActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".WSYActivity" />
        <activity android:name=".YouMinActivity" />
        <activity android:name=".BaeHoonActivity" />
        <activity android:name=".BaeHoonActivity2" />
        <activity android:name=".BaeHoonActivity3" />
        <activity android:name=".Maincheck" />

        <service
            android:name=".Notification.MyFirebaseIdService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name=".Notification.MyFirebaseMessaging"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />
    </application>

</manifest>